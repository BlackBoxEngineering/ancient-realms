[ITEMDEF i_silk_strands]
DEFNAME=i_silk_strands
ID=6915
NAME=Silk Strand
VALUE=30
TYPE=t_wool
WEIGHT=3
DYE=1
CATEGORY=Ancientrealms
SUBSECTION=Tailor
DESCRIPTION=Silk (Strand)

ON=@Create
COLOR=color_cloth_silk

ON=@Targon_Item
IF (<SRC.TARG.BASEID> == i_spinning_wheel)
	IF (<SRC.INT> >= 50)
		IF (<SRC.TAILORING> >= 700)
			REMOVE
			SERV.NEWITEM=i_thread_silk
			NEW.AMOUNT=(<SRC.TARG.AMOUNT>)
			NEW.BOUNCE <SRC.UID>
			RETURN 1
		ELSE
			SRC.SMSG 70%+ Tailoring Needed!
			BOUNCE
			RETURN 1
		ENDIF
	ELIF ((<SRC.INT> <= 50) && (<SRC.TAILORING> >= 700)
		SRC.SMSG 50+ Intelligence Needed!
		BOUNCE
		RETURN 1
	ELSE
		SRC.SMSG 50+ Intelligence and 70%+ Tailoring Needed!
		BOUNCE
		RETURN 1
	ENDIF		
ENDIF	

ON=@GETHIT
	say The Silk Strands are destroyed
	REMOVE
	RETURN 1

ON=@SPELLEFFECT
	say The Silk Strands are destroyed
	REMOVE
	RETURN 1


[EOF]
