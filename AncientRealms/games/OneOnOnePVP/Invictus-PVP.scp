[ITEMDEF i_duel_exit]
DEFNAME=i_duel_exit
TYPE=t_eq_script
NAME=Duel Exit
LAYER=layer_special

ON=@CREATE
	ATTR=ATTR_NEWBIE|ATTR_DECAY|ATTR_MOVE_NEVER
	TIMER=12

ON=@TIMER
	REMOVE
	RETURN 1

[ITEMDEF i_duel_exitCounter]
DEFNAME=i_duel_exitCounter
TYPE=t_eq_script
NAME=Duel Exit Counter
LAYER=layer_special

ON=@CREATE
	ATTR=ATTR_NEWBIE|ATTR_DECAY|ATTR_MOVE_NEVER
	TIMER=2

ON=@TIMER
	IF <EVAL <CONT.FINDID.i_duel_exit.TIMER>> < 1
		CONT.FINDID.i_duel_exit.REMOVE
		CONT.REGION.SYSMESSAGE @55,,1 Leaving Pits!!
		f_EndDuel
		REMOVE
	ELSE
		CONT.REGION.SYSMESSAGE @1462,,1 <EVAL <CONT.FINDID.i_duel_exit.TIMER>>
		TIMER=1
	ENDIF
	RETURN 1
	
[ITEMDEF i_duel_resurrect]
DEFNAME=i_duel_resurrect
TYPE=t_eq_script
NAME=Duel Resurrect
LAYER=layer_special

ON=@CREATE
	ATTR=ATTR_NEWBIE|ATTR_DECAY|ATTR_MOVE_NEVER
	TIMER=1

ON=@TIMER
	CONT.RESURRECT
	CONT.SAY @1462 Looser!!
	REMOVE
	RETURN 1

[EVENTS e_PlayerDuel]
ON=@GETHIT
	IF <UID>=<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerOneUID>
		SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoScore=<EVAL <SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoScore>+<ARGN1>>
		SRC.SYSMESSAGE @1462,,1 Score: <EVAL <SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoScore>>
	ELIF <UID>=<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoUID>
		SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerOneScore=<EVAL <SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerOneScore>+<ARGN1>>
		SRC.SYSMESSAGE @1462,,1 Score: <EVAL <SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerOneScore>>
	ELSE
		//SERV.LOG EVENT e_PlayerDuel is on <NAME> <UID> and shouldnt be ?
	ENDIF

ON=@DEATH
	IF <UID>=<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerOneUID>
		NEWITEM i_duel_resurrect,1,<SERV.UID.<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerOneUID>>
		SERV.UID.<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoUID>.GO 1409,1710,20
		SERV.UID.<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoUID>.SAY @1462 Winner!!
		NEWITEM i_duel_exit,1,<SERV.UID.<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoUID>>
		NEWITEM i_duel_exitCounter,1,<SERV.UID.<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoUID>>
		SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoScore=<EVAL <SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoScore>-<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.TimePenalty>>
		SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoScore=<EVAL <SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoScore> + 200>
	
	ELIF <UID>=<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoUID>
		NEWITEM i_duel_resurrect,1,<SERV.UID.<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoUID>>
		SERV.UID.<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerOneUID>.GO 1404,1721,20
		SERV.UID.<SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerOneUID>.SAY @1462 Winner!!
