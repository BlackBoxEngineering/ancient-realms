    SRC.SYSMESSAGE @92,,1 You must Accept or Decline This Challenge
    MENU d_duelInvite

[FUNCTION DuelSelectPlayer]

IF <SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.CurrentGame>
	SYSMESSAGE @55,,1 Arena is currently in use.
	RETURN 1
ELSE
	IF (<SERV.UID.<ARGO>.UID> != <UID>)
		IF 	<SERV.UID.<ARGO>.ISPLAYER>
			EMOTE Offers <SERV.UID.<ARGO>.NAME> a Duel
			SERV.UID.<ARGO>.MENU d_duelInvite
		ELSE
			SYSMESSAGE @55,,1 Thats not worth dueling!
		ENDIF			
	ELSE
		SYSMESSAGE @55,,1 You want to fight yourself?
	ENDIF
ENDIF

