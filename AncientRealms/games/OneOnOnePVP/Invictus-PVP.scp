	SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoName=<SRC.NAME>
	SERV.UID.<VAR.WorldStonesDuelStoneUidBritain>.TAG0.PlayerTwoUID=<SRC.UID>
