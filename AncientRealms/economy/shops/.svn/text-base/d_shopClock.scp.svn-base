[DIALOG d_ShopClock BUTTON]
ONBUTTON=1
	IF <SERV.UID.<REGION.TAG.ShopControlUID>.TAG.ShopActivated>
		SERV.UID.<REGION.TAG.ShopControlUID>.TAG.ShopActivated=0
	ELSE
		SERV.UID.<REGION.TAG.ShopControlUID>.TAG.ShopActivated=1
	ENDIF
	SRC.SDIALOG d_ShopClock
	RETURN 1
ONBUTTON=2
	SRC.SYSMESSAGE @1462 Target the shops clock.
	SRC.ResetClock
	RETURN 1
ONBUTTON=3
	SRC.SYSMESSAGE @1462 Stand outside the shop and Target the shops clock.
	SRC.resetouterregions
	RETURN 1
ONBUTTON=4
	IF <SERV.UID.<SERV.UID.<REGION.TAG.ShopControlUID>.TAG.OwnersUID>.ISONLINE>
		SERV.UID.<SERV.UID.<REGION.TAG.ShopControlUID>.TAG.OwnersUID.GO <SRC.P>
	ELSE
		SRC.SYSMESSAGE @1462 <SERV.UID.<REGION.TAG.ShopControlUID>.TAG.OwnersName> is not online.
	ENDIF
	RETURN 1
ONBUTTON=5
	IF <SERV.UID.<SERV.UID.<REGION.TAG.ShopControlUID>.TAG.OwnersUID>.ISONLINE>
		SRC.GO <SERV.UID.<SERV.UID.<REGION.TAG.ShopControlUID>.TAG.OwnersUID>.P>
	ELSE
		SRC.SYSMESSAGE @1462 <SERV.UID.<REGION.TAG.ShopControlUID>.TAG.OwnersName> is not online.
	ENDIF
	RETURN 1

[EOF]